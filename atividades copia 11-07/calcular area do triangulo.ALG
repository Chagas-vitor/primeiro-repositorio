Algoritmo "calcular area"



Var

   altura, largura, area: real

Inicio

   escreva("Digite a largura: ")
   leia (largura)
   
   escreva("Digite a altura: ")
   leia(altura)
   
   area := altura * largura
   
   escreval
   
  escreval("A area total do triangulo é: ", area)
   

    

Fimalgoritmo