algoritmo "VerificarPositivo"

var
    numero: real

inicio
    escreva("Digite um número: ")
    leia(numero)

    se (numero > 0) então
        escreva("O número é positivo.")
        
     senao se entao
     escreva ("O numero é negativo")
        
        
        
        
    fimse
fimalgoritmo
